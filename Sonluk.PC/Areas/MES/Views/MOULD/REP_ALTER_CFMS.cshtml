                                var rstData = JSON.parse(data);
                                $('#Term_GZZXBH').empty();
                                $('#Term_GZZXBH').append(new Option("请选择", ""));
                                $.each(rstData, function (index, item) {
                                    $('#Term_GZZXBH').append(new Option(item.GZZXBH + "-" + item.GZZXMS, item.GZZXBH));
                                })
                                if (rstData.length == 1) {
                                    $('#Term_GZZXBH').val(rstData[0].GZZXBH);
