            function list_init_Term(order) {
                order = order || "";
                switch (order) {
                    case "AfterGC":
                        $('#Term_MOULD').empty();
                        $('#Term_MOULD').append(new Option("请选择", ""));
                        $.ajax({
                            type: "POST",
                            async: false,
                            url: "../MOULD/GZZXList",
                            data: {
                                GC: $('#Term_GC').val()
