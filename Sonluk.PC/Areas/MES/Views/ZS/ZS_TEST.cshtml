@{
    ViewBag.Title = "密封圈全检实时工况";
    Layout = "~/Views/Shared/_Layout_KB.cshtml";
}

<style>
    .t_table {
        display: table;
        border-collapse: separate;
        box-sizing: border-box;
        border-spacing: 2px;
        /*border-color: grey;*/
        margin: 0 auto;
    }

    .td_height {
        height: 18.5px;
    }

    .layui-form-radio {
        line-height: 28px;
        margin: 0px -15px 0px 0px;
        padding-right: 10px;
        cursor: pointer;
        font-size: 0;
    }

    .T_tr {
        width: 100%;
        height: 100%
    }

    @@media screen and (max-width: 1460px) {
        .fix {
            width: 1460px;
        }
    }

    input[type="radio"] {
        width: 20px;
        height: 20px;
        background-color: #FFFFFF;
        -webkit-appearance: none;
        border: 1px solid #c9c9c9;
        border-radius: 2px;
        outline: none;
    }

    input[type="radio"] {
        border-radius: 50%;
    }

        input[type="radio"]:checked {
            background-color: #008B8B;
            border-color: #008B8B;
            background-clip: content-box;
            padding: 2px;
        }
</style>

<div id="div_result" class="cd-pricing-container cd-full-width fix" style="padding-top:0px;height:100%">

    <div style="float:left;text-align: center;width:100%;">
        <span style="font-size:30px;height:40px;color:white">密封圈全检实时工况</span>
    </div>

    <div class="layui-footer" style="text-align:right" id="footer">
        <div class="layui-inline">
            <label class="layui-form-label">合计页：</label>
            <label class="layui-form-label" id="Text_Total" style="width:5px"></label>
            <label class="layui-form-label">当前页：</label>
            <label class="layui-form-label" id="Text_Curr" style="width:30px"></label>
        </div>
    </div>
</div>



@section scripts {
    @Scripts.Render("~/bundles/MES/ZS/ZS_TEST")
}








