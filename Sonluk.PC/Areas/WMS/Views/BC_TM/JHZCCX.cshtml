@{
    ViewBag.Title = "内销交货转储查询报表";
    Layout = "~/Views/Shared/_Layout_CRM.cshtml";
    
}
<script type="text/html" id="indexTpl">
    {{d.LAY_TABLE_INDEX+1}}
</script>

<script type="text/html" id="Tpl_RKZT">
    {{# if(d.RKZT == 1){ }}
    车间发出
    {{# }else if(d.RKZT == 2){ }}
    物流入库
    {{# }else { }}

    {{# } }}
</script>

<script type="text/html" id="Tpl_ISZC">
    {{# if(d.ISZC == 0){ }}
    否
    {{# }else { }}
    是
    {{# } }}
</script>

<script type="text/javascript" src="~/Scripts/layui2.2.5/extend/formSelects-v4.js"></script>
<link rel="stylesheet" type="text/css" href="~/Scripts/layui2.2.5/css/formSelects-v4.css">



<span class="layui-breadcrumb" lay-separator=">">
    <a>WMS查询报表</a>
    <a><cite>内销交货转储查询报表</cite></a>
</span>


<div id="div1" class="layui-form" style="margin-left:10px">
    <div id="div_height">


        <div class="layui-form-item">

            <button id="btn_dc" class="layui-btn">导出</button>
            <button id="btn_cx" class="layui-btn">查询</button>

        </div>


        <div class="layui-collapse">
            <div class="layui-colla-item">
                <h2 class="layui-colla-title">查询条件</h2>
                <div id="div_select_tiaojian" class="layui-colla-content layui-show">

                    <div class="layui-form-item">


                        <div class="layui-inline">
                            <label class="layui-form-label">交货单：</label>
                            <div class="layui-input-inline">
                                <input id="JHNO" type="text" autocomplete="off" placeholder="" class="layui-input">
                            </div>

                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">拣配单：</label>
                            <div class="layui-input-inline">
                                <input id="JPDNO" type="text" autocomplete="off" placeholder="" class="layui-input">
                            </div>

                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">物流码：</label>
                            <div class="layui-input-inline">
                                <input id="DXM" type="text" autocomplete="off" placeholder="" class="layui-input">
                            </div>

                        </div>
                    </div>

                    <div class="layui-form-item">
                        <div class="layui-inline">
                            <label class="layui-form-label">物料信息：</label>
                            <div class="layui-input-inline">
                                <input id="WLMS" type="text" autocomplete="off" placeholder="物料编码/物料描述" class="layui-input">
                            </div>

                        </div>

                        <div class="layui-inline">
                            <label class="layui-form-label">创建日期：</label>
                            <div class="layui-input-inline">
                                <input id="JLTIMEBEGIN" type="text" autocomplete="off" placeholder="" class="layui-input">
                            </div>
                        </div>

                        <div class="layui-inline">
                            <label class="layui-form-label">至：</label>
                            <div class="layui-input-inline">
                                <input id="JLTIMEEND" type="text" autocomplete="off" placeholder="" class="layui-input">
                            </div>
                        </div>

                        
                        <input type="checkbox" id="in_qxbs" lay-filter="in_qxbs" name="" title="未取消" lay-skin="primary" checked>

                    </div>


                </div>
            </div>
        </div>




    </div>
    <table id="table_WL" lay-filter="table_WL"></table>


</div>



@*<script src="~/Scripts/layui2.5.4/layui.all.js"></script>*@



@section scripts {
    @Scripts.Render("~/bundles/WMS/BC_TM/JHZCCX")
}