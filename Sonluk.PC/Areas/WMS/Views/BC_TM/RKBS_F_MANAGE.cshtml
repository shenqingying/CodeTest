            GC: GC,
            GZZXBH: GZZXBH,
            TMLIKE: TMLIKE,
            NOBILL: NOBILL,
            NOBILLMX: NOBILLMX,
            ERPNO: ERPNO,
            WLMS: WLMS,
            JLKSTIME: JLKSTIME,
            JLJSTIME: JLJSTIME
        };
        var APIHEADER = [];
        var cxlb = {
            KEY: "CXLB",
            VALUES: "3"
        }
        APIHEADER.push(cxlb);
        $.ajax({
            type: "POST",
            async: false,
            url: "../BC_TM/API_RETURN_STRING_json",
            data: {
                apihsm: "MES/TM/SELECT_BY_STAFFID",
                body: JSON.stringify(datastring),
                APIHEADER: JSON.stringify(APIHEADER)
            },
            success: function (data) {
                var resdata = JSON.parse(data);
                if (resdata.type == "S") {
                    var fyall = Math.ceil(resdata.data.length / all_fysl);
                    if (fyall > all_fy - 1) {
                    }
                    else {
                        all_fy = Number(fyall);
                    }
                    selectdata = resdata.data;
                    table.render({
                        done: function (res, curr, count) {
                            for (var i = 0; i < all_limits.length; i++) {
                                if (all_limits[i] >= res.data.length) {
                                    all_fysl = all_limits[i];
                                    break;
                                }
                            }
                            all_fy = curr;
                        },
                        height: 'full-' + "350",
                        elem: '#tb_tminfo',
                        data: resdata.data,
                        cols: [table_zd]
                        , page: {
                            limits: all_limits,
                            limit: all_fysl,
                            curr: all_fy
                        },
                    });
                }
                else {
                    layer.msg(resdata.messages[0].message);

                }
            }
        });
    }

