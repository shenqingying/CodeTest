@{
    Layout = "~/Views/Shared/_Layout_CRM.cshtml";
    ViewBag.Title = "查询喷码(生产)";

    string Staffid = "";
    if (Session["STAFFID"] != null)
    {
        Staffid = Session["STAFFID"].ToString();
    }
}
<script type="text/html" id="indexTpl">
    {{d.LAY_TABLE_INDEX+1}}
</script>

<script type="text/html" id="suoshu">
    {{# d.Zbon * d.Zpks }}

</script>



<script type="text/html" id="bar">
    <a class="layui-btn layui-btn-xs" lay-event="watch">查看</a>
    <a class="layui-btn layui-btn-xs layui-btn-danger" lay-event="delete">删除</a>
</script>



<span class="layui-breadcrumb" lay-separator=">">
    <a href="../Public/Index1">首页</a>
    <a>防窜货管理</a>
    <a><cite>查询喷码(生产)</cite></a>
</span>
<br />
<br />

<div class="layui-form layui-form-pane">
    <div class="layui-form-item">

        <button id="btn_cx" class="layui-btn" lay-filter="formDemo">查询</button>
        <button id="btn_back" class="layui-btn" lay-filter="formDemo" style="display:none;margin-left:0;">返回</button>
        <button id="btn_update" class="layui-btn" lay-filter="formDemo" style="margin-left:0;">数据同步</button>
    </div>

    <div class="layui-collapse">
        <div class="layui-colla-item">
            <h2 class="layui-colla-title">查询条件</h2>
            <div id="div_select_tiaojian" class="layui-colla-content layui-show">

                <div class="layui-form-item">
                    <label class="layui-form-label" style="width:120px;text-align:right;">工单：</label>
                    <div class="layui-input-inline">
                        <input id="workid" type="text" autocomplete="off" class="layui-input">

                    </div>
                </div>

                <div class="layui-form-item">
                    <label class="layui-form-label" style="width:120px;text-align:right;">物料号：</label>
                    <div class="layui-input-inline">
                        <input id="Material" type="text" autocomplete="off" class="layui-input">

                    </div>

                    <label class="layui-form-label" style="width:120px;text-align:right;">物料号描述：</label>
                    <div class="layui-input-inline">
                        <input id="Material_Name" type="text" autocomplete="off" class="layui-input">

                    </div>
                </div>

                <div class="layui-form-item">
                    <label class="layui-form-label" style="width:120px;text-align:right;">日期唛：</label>
                    <div class="layui-input-inline">
                        <input id="begin_date" type="text" autocomplete="off" class="layui-input">

                    </div>

                    <label class="layui-form-label" style="width:120px;text-align:right;">至：</label>
                    <div class="layui-input-inline">
                        <input id="end_date" type="text" autocomplete="off" class="layui-input">

                    </div>
                </div>

                <div class="layui-form-item">
                    <label class="layui-form-label" style="width:120px;text-align:right;">箱码：</label>
                    <div class="layui-input-inline">
                        <input id="DXM" type="text" autocomplete="off" class="layui-input">

                    </div>
                </div>

                @*<div class="layui-form-item">
                        <label class="layui-form-label"style="width:120px;text-align:right;">托码：</label>
                        <div class="layui-input-inline">
                            <input id="TPM" type="text" autocomplete="off" class="layui-input">

                        </div>
                    </div>*@

            </div>
        </div>
    </div>

    <div id="div_result">
        <table id="result" lay-filter="result"></table>
    </div>
    <div id="div_resultMX" hidden>
        <table id="resultMX" lay-filter="resultMX"></table>
    </div>

</div>








<input id="staffid" hidden />

<script src="~/Scripts/jquery-1.8.2.js"></script>
<script src="~/Scripts/layui2.2.5/layui.all.js"></script>
<script>
    $(document).ready(function () {
        $("#staffid").val("@Staffid");

    });
</script>

@section scripts {
    @Scripts.Render("~/bundles/CRM/FCH/Select_PM")
}