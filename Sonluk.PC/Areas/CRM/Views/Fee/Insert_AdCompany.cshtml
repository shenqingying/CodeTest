@{
    Layout = "~/Views/Shared/_Layout_CRM.cshtml";
    ViewBag.Title = "新增广告公司";

    string Staffid = "";
    if (Session["STAFFID"] != null)
    {
        Staffid = Session["STAFFID"].ToString();
    }
}


<span class="layui-breadcrumb" lay-separator=">">
    <a href="../Public/Index1">首页</a>
    <a>广告管理</a>
    <a><cite>新增广告公司</cite></a>
</span>
<br />
<br />

<div id="div1" class="layui-form" style="margin-left:10px">
    <div class="layui-form-item">
        <div class="layui-input-inline">
            <button id="btn_save" class="layui-btn">保存</button>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">省份：</label>
            <div class="layui-input-inline">
                <select id="SF" lay-filter="SF">
                    <option value="0" selected="selected"></option>
                </select>
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">城市：</label>
            <div class="layui-input-inline">
                <select id="CS" lay-filter="CS">
                    <option value="0" selected="selected"></option>
                </select>
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">广告公司名称：</label>
            <div class="layui-input-inline">
                <input id="GGGSMC" type="text" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">联系电话：</label>
            <div class="layui-input-inline">
                <input id="TEL" type="text" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">地址：</label>
            <div class="layui-input-inline">
                <input id="ADDRESS" type="text" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">广告公司开户银行：</label>
            <div class="layui-input-inline">
                <input id="KHYH" type="text" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:170px;text-align:right;">广告公司开户账户：</label>
            <div class="layui-input-inline">
                <input id="KHZH" type="text" autocomplete="off" class="layui-input">
            </div>
        </div>
    </div>

</div>
<script src="~/Scripts/layui2.2.5/layui.all.js"></script>


@section scripts {
    @Scripts.Render("~/bundles/CRM/Fee/Insert_AdCompany")
}