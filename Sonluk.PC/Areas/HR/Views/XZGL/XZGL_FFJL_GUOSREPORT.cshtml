@{
    ViewBag.Title = "国税报表";
    Layout = "~/Views/Shared/_Layout_CRM.cshtml";
    string mypw = "";
    if (Session["MYPW"] != null)
    {
        mypw = Session["MYPW"].ToString();
    }
}
<span class="layui-breadcrumb" lay-separator=">">
    <a>HR薪资福利</a>
    <a><cite>发放记录</cite></a>
    <a><cite>国税报表</cite></a>
</span>
<div class="layui-form">
    <div class="layui-form-item">
        @*<button class="layui-btn" onclick="location.href = '@Url.Action("XZGL_FFJLLIST", "XZGL")'">返回</button>*@
        <button class="layui-btn" onclick="window.close();">返回</button>
        <button class="layui-btn" id="btn_daochu">导出</button>
        <button class="layui-btn" id="btn_find">查询</button>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">公司:</label>
            <div class="layui-input-inline">
                <select lay-filter="find_gs" id="find_gs"></select>
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">申报期间:</label>
            <div class="layui-input-inline">
                <input type="text" id="find_monthks" autocomplete="off" class="layui-input" readonly="readonly" value="@DateTime.Now.ToString("yyyy-MM")">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">姓名：</label>
            <div class="layui-input-inline">
                <input type="text" id="find_gh" autocomplete="off" class="layui-input" placeholder="（姓名/工号/证件号码）" onkeypress="if(window.event.keyCode==13) this.blur()">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">个税类别：</label>
            <div class="layui-input-inline">
                <select xm-select="find_gslb" id="find_gslb"></select>
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">来源:</label>
            <div class="layui-input-inline">
                <select lay-filter="find_ly" id="find_ly"></select>
            </div>
        </div>
    </div>
    <table class="layui-hide" id="tb_guosreport" lay-filter="tb_guosreport"></table>
</div>
<div id="div_mypw" class="layui-form" hidden>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label1">密钥密码:</label>
            <div class="layui-input-inline1">
                <input type="password" id="myinfo_mypw" autocomplete="off" class="layui-input" onkeypress="if(window.event.keyCode==13) this.blur()">
            </div>
        </div>
    </div>
</div>
<div hidden>
    <input type="text" id="bl_mypw" value="@mypw">
</div>
@section scripts {
    @Scripts.Render("~/bundles/HR/XZGL/XZGL_FFJL_GUOSREPORT")
}
