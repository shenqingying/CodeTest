@using Sonluk.UI.Model.S5.SY_DICTService;
@using Sonluk.UI.Model.HR.SY_DEPTService;
@{
    Layout = "~/Views/Shared/_Layout-weui.cshtml";
    ViewBag.Title = "自动打分";
    Session["TITLENAME"] = "自动打分";

    Sonluk.UI.Model.S5.SY_DICTService.FIVES_SY_DICT[] SHOWNAME = ViewBag.SHOWNAME;
    HR_SY_DEPT_SELECT depArr = ViewBag.DEPList;
}
<div class="weui-btn-area">

    <button class="weui-btn weui-btn_primary" id="btn_insert" style="display:initial;margin:0 0;">批量生成</button>
    @*<button class="weui-btn weui-btn_primary" id="btn_cx" style="width:49%;display:initial;margin:0 0;float:right;">查询</button>*@

</div>
<br />
<div class="weui-cells weui-cells_form" style="margin-top:0;">
    <div class="weui-cell weui-cell_select weui-cell_select-after">
        <div class="weui-cell__hd">
            <label for="" class="weui-label" style="text-align:left">部门  ：</label>
        </div>
        <div class="weui-cell__bd">
            <select id="select_DEPT" class="weui-select">

                @*<option value="0" >请选择</option>*@
                @if (depArr.MES_RETURN.TYPE.Equals("S"))
                {
                    for (int i = 0; i < depArr.HR_SY_DEPT_LIST.Length; i++)
                    {
                        if (@depArr.HR_SY_DEPT_LIST[i].DEPTID == 11)
                        {
                            <option value="@depArr.HR_SY_DEPT_LIST[i].DEPTID" selected="selected">@depArr.HR_SY_DEPT_LIST[i].DEPTNAME</option>
                        }
                    }
                }
            </select>
        </div>
    </div>
    <div class="weui-cell weui-cell_select weui-cell_select-after">
        <div class="weui-cell__hd">
            <label for="" class="weui-label" style="text-align:left">检查点 ：</label>
        </div>
        <div class="weui-cell__bd">
            <select id="select_OPINTMS" class="weui-select">
                <option value="0" selected>全选</option>
            </select>
        </div>
    </div>
    <div class="weui-cell weui-cell_select weui-cell_select-after">
        <div class="weui-cell__hd">
            <label for="" class="weui-label" style="text-align:left">点检人员：</label>
        </div>
        <div class="weui-cell__bd">
            <select id="select_SHOWNAME" class="weui-select">
                <option value="0" selected="selected">请选择</option>
                @for (int i = 0; i < SHOWNAME.Length; i++)
                {
                    <option value="@SHOWNAME[i].DICID">@SHOWNAME[i].DICNAME</option>
                }
            </select>
        </div>
    </div>

    <div class="weui-cell weui-cell_active weui-cell_access" id="showDatePicker">
        <div class="weui-cell__hd">
            <label for="" class="weui-label" style="text-align:left">日期：</label>
        </div>
        <div class="weui-cell__bd" id="div_test"></div>
        <div class="weui-cell__ft"></div>
    </div>
    <div class="weui-cell">
        <div class="weui-cell__hd"><label class="weui-label" style="text-align:left;width:125px">时间区间(分)：</label></div>
        <div class="weui-cell__bd weui-cell_primary" style="width:100px">
            <input id="insert_FirstNum" class="weui-input" type="text" placeholder="" style="width:100px">
        </div>
        <div class="weui-cell__hd"><label class="weui-label" style="width:85px">-</label></div>
        <div class="weui-cell__bd weui-cell_primary" style="width:100px">
            <input id="insert_SecondNum" class="weui-input" type="text" placeholder="" style="width:100px">
        </div>
    </div>
</div>




