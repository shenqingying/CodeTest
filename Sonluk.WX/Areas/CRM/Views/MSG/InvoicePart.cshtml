@using Sonluk.UI.Model.CRM.MSG_INVOICEService;
@{
    ViewBag.Title = "InvoicePart";
    CRM_MSG_INVOICEList[] data = ViewBag.data;
}


@for (int i = 0; i < data.Length; i++)
{

    <div class="weui-form-preview">
        <div class="weui-form-preview__hd">
            <em class="weui-form-preview__value">@data[i].MC</em>
        </div>
        <div class="weui-form-preview__bd">
            <p>
                <label class="weui-form-preview__label">发票号码</label>
                <span class="weui-form-preview__value">@data[i].FPHM</span>
            </p>
            <p>
                <label class="weui-form-preview__label">发票数量</label>
                <span class="weui-form-preview__value">@data[i].FPSL</span>
            </p>
            <p>
                <label class="weui-form-preview__label">快递单号</label>
                <span class="weui-form-preview__value">@data[i].KDDH</span>
            </p>
            <p>
                <label class="weui-form-preview__label">寄送日期</label>
                <span class="weui-form-preview__value">@data[i].JSRQ</span>
            </p>

        </div>
    </div>
}