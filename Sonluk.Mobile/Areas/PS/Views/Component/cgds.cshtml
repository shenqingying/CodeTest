@{
    ViewBag.Title = "采购入库";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="layui-body">
    <div class="layui-row">
        <div class="layui-form-item">
            <div class="layui-input-block-title">
                <input type="text" name="in_bl" placeholder="请扫描网络号" autocomplete="off" class="layui-input" id="in_bl" onkeypress="if(window.event.keyCode==13) this.blur()">
            </div>
        </div>
    </div>
    <div id="cgdslist">
        <label class="layui-form-label" name="in_AUFNR" id="in_AUFNR"></label>
    </div>

    <div class="layui-footer" align="right">
        <button class="layui-btn" id="btn_cwb">长文本</button>
        <button class="layui-btn" id="btn_back">返回</button>
        <button class="layui-btn" id="save">保存</button>
        <button class="layui-btn" id="clear">取消</button>
    </div>
</div>



@section scripts {
    @Scripts.Render("~/bundles/PS/cgds")
}
