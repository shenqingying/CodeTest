@{
    ViewBag.Title = "托盘组合码新增";
    Layout = "~/Views/Shared/_Layoutbig.cshtml";
}

<script type="text/html" id="bar">
    <a class="layui-btn_table_min layui-btn-xs layui-btn-danger" lay-event="delete">删除</a>
</script>
<script type="text/html" id="indexTpl">
    {{d.LAY_TABLE_INDEX+1}}
</script>
<script type="text/html" id="tp_Tpl">
    {{# if(d.TPNO != ""){ }}
    <a style="font-size:x-large;">{{d.TPNO}}</a>
    {{# } }}
</script>

<div class="layui-body">
    <div class="layui-row">
        <div class="layui-form-item">
            <div class="layui-input-block-title">
                <input type="text" id="tm_tpm_sm" placeholder="请扫描托盘编号" autocomplete="off" style="height:50px" class="layui-input" onkeypress="if(window.event.keyCode==13) this.blur()">
            </div>
        </div>
    </div>
    <div id="table_div" hidden>
        <table class="layui-hide" id="tb_TPINFO" lay-filter="tb_TPINFO"></table>
    </div>
    <div id="div_foot" class="layui-footer" align="right">
        <button id="btn_sc" class="layui-btn">生成</button>
        <button id="btn_qx" class="layui-btn">取消</button>
    </div>

</div>
@section scripts {
    @Scripts.Render("~/bundles/MES/TPM/TPZHNO_ADD")
}